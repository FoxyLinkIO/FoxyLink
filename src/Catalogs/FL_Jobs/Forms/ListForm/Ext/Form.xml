			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
    Jobs.Ref,
    Jobs.DeletionMark,
    Jobs.Code,
    Jobs.CreatedAt AS CreatedAt,
    Jobs.ExpireAt AS ExpireAt,
    Jobs.Invoke,
    Jobs.Isolated,
    Jobs.MethodName,
    Jobs.Priority,
    Jobs.State,
    Jobs.Transactional
FROM
    Catalog.FL_Jobs AS Jobs
WHERE
    Jobs.State = &amp;State
    </QueryText>
				<MainTable>Catalog.FL_Jobs</MainTable>
